<odoo>
    <!--hanya di create saat install kalau bernilai "1"-->
    <!--    record ini hanya di-create saat install-->
    <record id="sequence_kecantikan_pasien_seq" model="ir.sequence">
        <field name="name">Pasien sequence</field>
        <field name="code">kecantikan.pasien</field>
        <field name="prefix">PAS</field>
        <field eval="3" name="padding"/> <!-- eval itu untuk number, klo name untuk string-->
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="company_id"/>
        <field eval="True" name="use_date_range"/>
    </record>

