<odoo>
    <!--hanya di create saat install kalau bernilai "1"-->
    <!--    record ini hanya di-create saat install-->
    <record id="sequence_nilai_mahasiswa_seq" model="ir.sequence">
        <field name="name">Mahasiswa sequence</field>
        <field name="code">nilai.mahasiswa</field>
        <field name="prefix">MHS/%(range_year)s/</field>
        <field eval="3" name="padding"/> <!-- eval itu untuk number, klo name untuk string-->
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="company_id"/>
        <field eval="True" name="use_date_range"/>
    </record>

    <record id="sequence_nilai_matkul_seq" model="ir.sequence">
        <field name="name">Score matkul</field>
        <field name="code">nilai.matkul</field>
        <field name="prefix">MK/%(range_year)s/</field>
        <field eval="3" name="padding"/>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="company_id"/>
        <field eval="True" name="use_date_range"/>
    </record>
</odoo>
